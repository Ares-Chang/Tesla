    /**
     * Test drive forms / Lead creation from order form.
     *
     * Form:
     *   - tesla_order_form
     * Url:
     *   - /order
     * Called from:
     *   - tesla_payment_forms.crm.js
     */
    TeslaAnalytics.OrderForm = (function () {

        var _formType = 'Order Form';

        return {
            /**
             * Lead creation from order form: order-form-lead-creation.
             *
             * @param string uuid Uuid.
             */
            interactionOrderFormLeadCreation: function (uuid) {
                TeslaAnalytics.Helpers.setUuid(uuid);
                TeslaAnalytics.Helpers.eventFormInteraction('order-form-lead-creation', _formType, 'SF Lead Creation');
            },

            interactionShowApplePayButton: function () {
                TeslaAnalytics.Helpers.eventApplePayInteraction('impression');
            },

            interactionClickApplePayButton: function () {
                TeslaAnalytics.Helpers.eventApplePayInteraction('click');
            }

        };
    }());

    /**
     * Tesla Account PW milestones page interactions
     *
     * Called from:
     *   - final_confirmation.js
     *   - tesla_cancellation.js
     *   - tesla_edit_order_energy.js
     */
    TeslaAnalytics.Milestones = (function () {

        return {
            interactionClickConfirmOrderReccomended: function () {
                TeslaAnalytics.Helpers.interactionClickConfirmOrder('recommended');
            },
            interactionClickConfirmOrderAlternate: function () {
                TeslaAnalytics.Helpers.interactionClickConfirmOrder('alternate');
            },
            interactionClickBackToEditOrder: function () {
                TeslaAnalytics.Helpers.interactionClickBackToEditOrder('predelivery-powerwall');
            },
            interactionClickSubmitCancel: function () {
                var reason_to_cancel = $('#edit-reason')[0].value;
                TeslaAnalytics.Helpers.interactionClickSubmitCancel('predelivery-powerwall', reason_to_cancel);
            },
            interactionEditOrderClickBack: function () {
                TeslaAnalytics.Helpers.interactionEditOrderClickBack('predelivery-powerwall');
            },
            interactionEditOrderClickSave: function () {
                TeslaAnalytics.Helpers.interactionEditOrderClickSave('predelivery-powerwall');
            },

        };
    }());

