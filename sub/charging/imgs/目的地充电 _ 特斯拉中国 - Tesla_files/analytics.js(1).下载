    /**
     * Analytics for the marketing campaign content type
     *   - /campaign/*
     * Called from:
     *   - tesla_drive_to_believe_form.general_marketing_form.inc
     */
    TeslaAnalytics.MarketingCampaignContent = (function () {
        var _formType = 'marketing-campaign-content: ';
        return {
            /**
             * view-open.
             *
             * @param {string} campaign_name the Campaign name
             */
            interactionViewOpen: function (campaign_name) {
                TeslaAnalytics.Helpers.interactionViewOpen(_formType + campaign_name);
            },

            /**
             * @param {string} uuid Uuid
             * @param {string} campaign_name The Campaign name
             */
            interactionPersonalInfoComplete: function (uuid, campaign_name) {
                TeslaAnalytics.Helpers.setUuid(uuid);
                TeslaAnalytics.Helpers.interactionPersonalInfoComplete(_formType + campaign_name);
            },

            /**
             * @param {string} uuid Uuid
             * @param {string} campaign_name the campaign name
             */
            interactionConfirmThankYou: function (uuid, campaign_name) {
                TeslaAnalytics.Helpers.setUuid(uuid);
                TeslaAnalytics.Helpers.interactionConfirmThankYou(_formType + campaign_name);
            }
        };
    }());

